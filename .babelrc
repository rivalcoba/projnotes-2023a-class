{
  "presets" : [
    [
      "minify",{
        "builtIns" : false,
        "evaluate" : false,
        "mangle" : false
      }
    ],
    [
      "@babel/preset-env",{
        // Los targets son las plataformas
        // Donde se ejecutara el codigo final
        "targets" : { "node" : "current" }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver", {
        "root": ["./"],
        "alias": {
          "@server" : "./server"
        }
      }
    ]
  ]
}